#include <stdio.h>
int main() {
    int i, j,k,a[3][3],b[3][3],r[3][3];    
    printf("Enter the first elements\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            scanf("%d", &a[i][j]);
        }
    }
    printf("Enter the second elements\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            scanf("%d", &b[i][j]);}
    }
    for (i = 0; i < 3; i++) {
    for (j = 0; j < 3; j++){
        r[i][j]=0;
         for (k=0;k<3;k++){
        r[i][j]+=a[i][k]*b[k][j];
        }}}
            printf("The multiplication of 3x3 is:\n");
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            printf("%d ", r[i][j]);
        }
        printf("\n");
    }
    return 0;
}
